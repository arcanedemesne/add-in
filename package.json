{
  "name": "add-in",
  "version": "1.0.0",
  "description": "This is a sample project to turn into an npm package!",
  "scripts": {
    "main": "build/bundle.js",
    "prestart": "babel-node tools/prestart.js",
    "start": "npm-run-all --parallel open:src",
    "open:src": "babel-node tools/server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean-build": "npm run remove-build && mkdir build",
    "remove-build": "node_modules/.bin/rimraf ./build",
    "prebuild": "npm-run-all clean-build",
    "build": "babel-node tools/build.js",
    "postbuild": "babel-node tools/postbuild.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arcanedemesne/add-in.git"
  },
  "keywords": [
    "sample",
    "add-in",
    "casing"
  ],
  "author": "Jennifer Allen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/arcanedemesne/add-in/issues"
  },
  "homepage": "https://github.com/arcanedemesne/add-in#readme",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.11.5",
    "colors": "^1.1.2",
    "npm-run-all": "^2.3.0",
    "open": "0.0.5",
    "rimraf": "^2.5.4",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.12.2"
  }
}
